{
  "manifest_version": 3,
  "name": "Safe News",
  "description": "A web scraper for generating statistics about news data.",
  "version": "1.0",
  "action": {
    "default_popup": "./index.html",
    "default_icon": "./img/logo.png"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./jquery-3.7.1.min.js", "./content.js"]
      }
    ]
}
